@model Odie.Models.ViewModels.EditorViewModel
@if (ViewContext.RouteData.Values["action"].ToString() == "Editor")
{
<ul class="nav navbar-nav navbar-left editor-toolbar">
    <li>
        <button id="tb-save" class="btn btn-primary navbar-btn"><span class="glyphicon glyphicon-floppy-disk" aria-hidden="true"></span> Save</button>
    </li>

    <li>
        <button id="tb-preview" class="btn btn-primary navbar-btn"><span class="glyphicon glyphicon-eye-open" aria-hidden="true"></span></button>
    </li>

    <li>
        <button id="tb-ws" class="btn btn-warning navbar-btn"><span class="glyphicon glyphicon-cloud" aria-hidden="true"></span></button>
    </li>

    <li>
        <a id="tb-users" class="btn btn-link" role="button"><span class="glyphicon glyphicon-flash glyphicon-ring" aria-hidden="true"></span></a>
    </li>

    <li>
        <a id="tb-files" class="btn btn-link" role="button" asp-area="" asp-controller="Files" asp-action="Index" asp-route-id="@Model.File.ProjectID" title="Back to Files Index">Close</a>
    </li>
</ul>
}